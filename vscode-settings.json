{
    "keyboard.dispatch": "keyCode",
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
      {
        "before": ["j", "j"],
        "after": ["<Esc>"]
      }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
      {
        "before": ["<leader>", "d"],
        "after": ["d", "d"]
      },
      {
        "before": ["<C-n>"],
        "commands": [":nohl"]
      },
      {
        "before": ["<leader>", "w"],
        "commands": [":w"]
      },
      {
        "before": ["<leader>", "q"],
        "commands": [":q"]
      }
    ],
    "vim.leader": ",",
    "vim.handleKeys": {
      "<C-a>": false
    },
    "kite.showWelcomeNotificationOnStartup": false,
    "window.zoomLevel": -1
}
